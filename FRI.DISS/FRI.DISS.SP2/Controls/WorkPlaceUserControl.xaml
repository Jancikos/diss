<UserControl x:Class="FRI.DISS.SP2.Controls.WorkPlaceUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FRI.DISS.SP2.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="200">
    <GroupBox x:Name="_grb_Header" Header="Workplace" MinWidth="150" >
        <Grid>
            <local:ObjednavkaUserControl x:Name="_uc_Objednavka" />
            <TextBlock x:Name="_txt_NoObjednavka" Text="Empty"  Visibility="Collapsed" />
        </Grid>
    </GroupBox>
</UserControl>
